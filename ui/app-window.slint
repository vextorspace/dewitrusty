import {HorizontalBox, VerticalBox} from "std-widgets.slint";

export struct ListItem {
    text: string,
}

export component AppWindow inherits Window {

    in-out property <[ListItem]> items;

    HorizontalBox {
        for item in items : VerticalBox {
            Text {
                text: item.text;
            }
        }
    }
}